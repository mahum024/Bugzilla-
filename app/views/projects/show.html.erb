<div class="ProjectShow">

  <p>
    <h1><strong>Name:   <span class="mx-5"> <%= @project.name %></span></strong></h1>
  </p>

  <h1 class="text-center my-5">Users IN Project</h1>
    
    <p class="my-5 mx-5"> 
      <strong class="mx-5">Name  :</strong>
      <span class="mx-5"> Role </span>   
    </p>
    
    <% @users.each do |user| %>
      <p class="my-5 mx-5"> 
        <strong class="my-5 mx-5" > <%= user.name %>  </strong>
        <span class="mx-5"> <%= user.user_type%> </span>
      </p>
    <% end %>
   
  <div class=" text-center">

    <% if current_user.user_type == 'manager' %>
      <%= link_to 'Edit', edit_project_path(@project) %> |
    <% end %>
    <%= link_to 'Back', projects_path %>
  </div>
</div>
