
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% user.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <% if in_project %>
        <td><%= link_to 'Remove', project_remove_user_path(@project, user)%></td>  
        <%  else %>
        <td><%= link_to 'Add', project_add_user_path(@project, user)%></td>  
         <% end %>
     </tr>
    <%end%>
     
  </tbody>
</table>

